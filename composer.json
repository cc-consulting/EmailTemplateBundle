    "description": "Twig extension for quick integration of email templates that you manage in the database",
    "keywords": ["symfony", "form-type", "email-templates", "form", "twig"],
    "homepage": "http://github.com/cc-consulting/EmailTemplateBundle",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Michael Langford",
            "email": "michael.langford@ccconsulting.jp"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/form": "~2.2",
        "friendsofsymfony/jsrouting-bundle": "~1.1",
        "egeloen/ckeditor-bundle": "2.*"
    },
    "autoload": {
        "psr-0": { "CCC\\EmailTemplateBundle": "" }
    },
    "target-dir": "CCC/EmailTemplateBundle",
    "minimum-stability": "dev"
}
